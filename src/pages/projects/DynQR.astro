---
import ProjectLayout from '@layouts/ProjectLayout.astro';

const title = 'DynQR';
const about = `DynQR is a simple and easy to use Dynamic QR code generator. It
allows you to generate QR codes for any text or URL. It also
allows you to download the QR code as an image.`;
const githubUrl = 'https://github.com/gamersi/DynQR';
const features = [
    'Simple and easy to use',
    'Generates QR codes for any text or URL',
    'Allows you to download the QR code as an image',
    'Allows for editing the content of the QR code',
    'Allows for deactivating the QR code',
    'Free and open source'
];
const technologies = [
    'React',
    'Node.js',
    'Express'
];
---

<ProjectLayout 
    {title} 
    {about} 
    {githubUrl} 
    {features} 
    {technologies} 
/>
