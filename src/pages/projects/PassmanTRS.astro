---
import ProjectLayout from '@layouts/ProjectLayout.astro';

const title = 'PassmanTRS';
const about = `PassmanTRS is a password manager that is designed to be simple
and easy to use. It is built with Rust and Svelte.`;
const githubUrl = 'https://github.com/gamersi/PassmanTRS';
const features = [
    'Simple and easy to use',
    'Secure',
    'Fast',
    'Cross-platform',
    'Basic password Features',
    'Password Generator',
    'Free and Open Source'
];
const technologies = [
    'Rust',
    'Svelte',
    'Tauri',
    'PBKDF2',
    'bcrypt',
    'AES-256-GCM'
];
---

<ProjectLayout 
    {title} 
    {about} 
    {githubUrl} 
    {features} 
    {technologies} 
/>
