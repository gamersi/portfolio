---
import ProjectLayout from '@layouts/ProjectLayout.astro';

const title = 'WordleWis';
const about = `A simple german wordle game written in JavaScript and jQuery.`;
const githubUrl = 'https://github.com/gamersi/WordleWis';
const demoUrl = 'https://gamersi.github.io/WordleWis/';
const features = [
    'Simple',
    'Free and open source'
];
const technologies = [
    'Vanilla JavaScript',
    'jQuery'
];
---

<ProjectLayout 
    {title} 
    {about} 
    {githubUrl} 
    {demoUrl}
    {features} 
    {technologies} 
/>
